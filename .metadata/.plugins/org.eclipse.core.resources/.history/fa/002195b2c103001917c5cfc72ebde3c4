package view;

public class ImageMove {
	private int dx;
	private int dy;
	private int rotation;
	public ImageMove(int dx, int dy) {
		this.dx = dx;
		this.dy = dy;
		this.rotation = 0;
	}
	public int getDx() {
		return dx;
	}
	public int getDy() {
		return dy;
	}
	public int getRotation() {
		return rotation;
	}
}
