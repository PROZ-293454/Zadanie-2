package model;

public class BallModel {
	
	public enum Color {BLACK, WHITE};
    private Color color;

    private int posX;
	private int posY;

	
	public BallModel(Color color, int posX, int posY) {
		this.color = color;
		this.posX = posX;
		this.posY = posY;
	}
	
	public int getPosX() {
		return posX;
	}

	public int getPosY() {
		return posY;
	}

	public Color getColor() {
		return color;
	}
	
}
